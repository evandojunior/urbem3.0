{% if admin.canRevogar(object) %}
    <a href="{{ admin.generateObjectUrl('revogar', object) }}" data-toggle="tooltip" data-placement="bottom" title="Revogar" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-window-close-o fa-lg" aria-hidden="true"></i>
    </a>
{% else %}
    <a href="#" data-toggle="tooltip" data-placement="bottom" title="Desoneração já revogada" class="grey-text transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-window-close-o fa-lg" aria-hidden="true"></i>
    </a>
{% endif %}