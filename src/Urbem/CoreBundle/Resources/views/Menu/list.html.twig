{% for modulo in menuModules %}
    {% set classStyle = " grey-text" %}
    {% set urlAction = "#@" %}

    {%  if modulo['route'] is not empty %}
        {% set classStyle = " blue-text text-darken-4" %}
        {% set urlAction = path(modulo['route']) %}
    {% endif %}

    <span data-tooltip="{{ modulo['title'] | trans }}">
        <li>
            <a href="{{ urlAction }}" class="btn-floating white">
                <i aria-hidden="true" class="material-icons {{ classStyle }}">{{ modulo['icon'] }}</i>
                <span class="sr-only">{{ modulo['title'] | trans }}</span>
            </a>
        </li>
    </span>
{% endfor %}

